public class User {
    private Long id;
    private String email;
    private String password;
    private String username;
    // геттеры и сеттеры
}

public class Review {
    private Long id;
    private User user;
    private String comment;
    private int rating;
    // геттеры и сеттеры
}

public class CoffeePlace {
    private Long id;
    private String name;
    private String address;
    private String description;
    // геттеры и сеттеры
}
